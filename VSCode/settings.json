{
    "workbench.colorCustomizations": {
        // "tab.activeBackground": "#ff0000", // tabs at the top
        // "activityBar.background": "#ff0000"
        "sideBar.background": "#131313", // the extensions side
        // "tab.activeBorder": "#ff0000", // tabs ke niche line
        "editor.background": "#131313", // editor screen ka bg
        "sideBarSectionHeader.background": "#111111",
        "titleBar.activeBackground": "#131313",
        "titleBar.inactiveBackground": "#131313",
        "tab.activeBackground": "#131313",
        "tab.inactiveBackground": "#131313",
        "tab.unfocusedActiveBackground": "#131313",
        "editorGroupHeader.tabsBackground": "#131313",
        "terminal.background": "#131313",
        "editorGutter.background": "#131313",
        "panel.background": "#131313"


    },

    "workbench.startupEditor": "none",
    "workbench.panel.defaultLocation": "bottom",
    "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.fontFamily": "'chalkboard'",
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.renderWhitespace": "none",
    "code-runner.runInTerminal": true,
    "terminal.integrated.fontWeightBold": "normal",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1,
    "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
    "vim.foldfix": true,
    "vim.neovimUseConfigFile": true,
    "code-runner.executorMap": {

        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName"
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "leetcode.defaultLanguage": "cpp",
    "leetcode.workspaceFolder": "/Users/akshatsharma/Developer/Programming/LeetCode",
    "editor.wordWrap": "on",
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.hint.commentDescription": false,
    "leetcode.hint.commandShortcut": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "html.format.wrapAttributes": "force-aligned",
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "workbench.iconTheme": "vikings-icon-theme",
    "git.autofetch": true,
    "editor.renderControlCharacters": false, // user profile top right section
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "http.proxySupport": "on",
    "discord.lowerDetailsIdling": "LazyPaloozing  ",
    "discord.removeTimestamp": true,
    "discord.largeImageIdling": "LazyPaloozing",
    "workbench.tree.indent": 25,
    "files.trimTrailingWhitespace": true,
    "editor.fontLigatures": true,
    "files.simpleDialog.enable": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.accessibilitySupport": "off",
    "terminal.integrated.fontWeight": "normal",
    "editor.renderLineHighlight": "none",
    "git.decorations.enabled": false,
    "scm.diffDecorationsGutterVisibility": "hover",
    "notebook.output.textLineLimit": 60,
    "notebook.output.scrolling": true,
    "editor.indentSize": 4,
    // "editor.fontWeight":"500",
    "javascript.suggest.autoImports": true,
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "breadcrumbs.enabled": false,
    "editor.lightbulb.enabled": "off",
    "notebook.compactView": false,
    "notebook.output.fontSize": 13,
    "notebook.diff.enablePreview": false,
    "notebook.diff.overviewRuler": true,
    "notebook.output.fontFamily": "'Roboto Mono','mechanical'",
    "workbench.sideBar.location": "right",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "jupyter.askForKernelRestart": false,
    "editor.fontSize": 17,
    "workbench.colorTheme": "Ronin",
    "workbench.activityBar.location": "top",
    "editor.stickyScroll.maxLineCount": 1,
    "editor.stickyScroll.scrollWithEditor": false,
    "workbench.tree.enableStickyScroll": false,
    "terminal.integrated.stickyScroll.enabled": true,
    "editor.guides.bracketPairs": "active",
    "jupyter.themeMatplotlibPlots": true,
    "workbench.editor.enablePreview": false,
    "workbench.statusBar.visible": false,
    "gitlens.graph.minimap.additionalTypes": [
        "localBranches",
        "stashes",
        "remoteBranches"
    ],
    "editor.guides.indentation": false,
    "terminal.integrated.fontFamily": "'JetBrains Mono'",
    "terminal.integrated.fontSize": 16,
    "better-yaml.directives":true,
    "better-yaml.flowCollectionPadding": true,
    "terminal.integrated.lineHeight": 1.3,
    "security.workspace.trust.untrustedFiles": "open"

}
